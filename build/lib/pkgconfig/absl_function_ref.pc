prefix=/usr/local
exec_prefix=${prefix}
libdir=/usr/local/lib
includedir=/usr/local/include

Name: absl_function_ref
Description: Abseil function_ref library
URL: https://abseil.io/
Version: 20240722
Requires: absl_base_internal = 20240722, absl_core_headers = 20240722, absl_any_invocable = 20240722, absl_meta = 20240722
Libs: -L${libdir}  
Cflags: -I${includedir} -Wno-float-conversion -Wno-implicit-float-conversion -Wno-implicit-int-float-conversion -Wno-unknown-warning-option -DNOMINMAX
