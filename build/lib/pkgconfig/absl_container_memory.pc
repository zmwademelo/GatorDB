prefix=/usr/local
exec_prefix=${prefix}
libdir=/usr/local/lib
includedir=/usr/local/include

Name: absl_container_memory
Description: Abseil container_memory library
URL: https://abseil.io/
Version: 20240722
Requires: absl_config = 20240722, absl_memory = 20240722, absl_type_traits = 20240722, absl_utility = 20240722
Libs: -L${libdir}  
Cflags: -I${includedir} -Wno-float-conversion -Wno-implicit-float-conversion -Wno-implicit-int-float-conversion -Wno-unknown-warning-option -DNOMINMAX
